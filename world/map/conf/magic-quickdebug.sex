(SPELL (NONMAGIC SILENT) eventmgr "%event" (STRING target)
    (=>
        (GUARD
            (OR (CATALYSTS "DevelopersCap") (CATALYSTS "GMCap")))
        (EFFECT
            (SCRIPT "{\ncallfunc \"eventManagerHandler\";\n}"))))
