"--------------------------------------------------------------------------------"
"Quickdebug Spells"
"--------------------------------------------------------------------------------"
(SPELL (NONMAGIC SILENT) debug0 "#debug" ()
    (EFFECT
        (SCRIPT "{if(!debug&&(getgmlevel()<99))end;callfunc \"Debug\";}")))

(SPELL (NONMAGIC SILENT) debug1 "#numa" ()
    (EFFECT
        (SCRIPT "{if(!debug&&(getgmlevel()<99))end;callfunc \"SuperDebug\";}")))
